              var self = d3.select(this);
              self.text(null);
              self.append("tspan")
                .attr("x", x)
                .attr("dy", offset)
                .text(lines[0]);
              self.append("tspan")
                .attr("x", x)
                .attr("dy", lineHeight + "em")
                .text(lines[1]);
