                      var greenBegin = "#76b900";
                      var greenEnd = "#c8e399";
                      if (isDark) {
                        greenEnd = "#3b5d00";
                      }
                      var greenScale = d3.interpolateLab(greenBegin, greenEnd);

                      var numGreens = 0;
                      for (var i = 0; i < data.length; i++) {
                        if (data[i].class == "green") {
                          numGreens++;
